<scene>
  <description text="Ningna"/>

  <maxsimfreq max="500.0"/>
  <duration time="10.0"/>
  <integrator type="symplectic-euler" dt="0.01"/>
  <particle m="1.0" px="0.0" py="0.0" vx="0.0" vy="0.0" fixed="1"/>
  <particle m="1.0" px="0.0" py="-1.0" vx="0.0" vy="0.0" fixed="1"/>  
  <particle m="1.0" px="4.0" py="0.0" vx="0.0" vy="0.0" fixed="1"/>
  <particle m="1.0" px="4.0" py="-1.0" vx="0.0" vy="0.0" fixed="1"/>
        
  <particle m="1.0" px="1.0" py="-0.5" vx="0.0" vy="0.0" fixed="0"/>  
  <particle m="1.0" px="2.0" py="-0.5" vx="0.0" vy="0.0" fixed="0"/>  
    
  <simplegravity fx="0.0" fy="-2.0"/>
    
  <!--  edges -->
  <edge i="0" j="1"/>
  <edge i="2" j="3"/>
    
  <edge i="0" j="4"/>
  <edge i="1" j="4"/>

  <edge i="4" j="5"/>
  
  <edge i="2" j="5"/>
  <edge i="3" j="5"/>
 
     
  <!--  springs -->
  <springforce edge="2" k="2000.0" l0="1.0" />
  <springforce edge="3" k="2000.0" l0="1.0" />
  <springforce edge="4" k="2000.0" l0="1.0" />
  <springforce edge="5" k="2000.0" l0="1.0" />
  <springforce edge="6" k="2000.0" l0="1.0" />
</scene>
